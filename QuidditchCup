/*
 * Quidditch Cup
 */

function hasEnoughPlayers(some_team){
    if (some_team.length > 6){
        return true;
    }
    else {
        return false;
    }
}

var team = ["Oliver Wood", "Angelina Johnson", "Katie Bell", "Alicia Spinnet", "George Weasley", "Fred Weasley", "Harry Potter"];
console.log(hasEnoughPlayers(team));
